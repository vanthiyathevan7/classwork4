#include <stdio.h>
#include <stdlib.h>

typedef struct {
    char name[50];
    int age;
    float height;
} Person;

int main() {
    Person person = {"John", 30, 1.8};
    printf("Name: %s\n", person.name);
    printf("Age: %d\n", person.age);
    printf("Height: %.2f meters\n", person.height);

    return 0;
}
